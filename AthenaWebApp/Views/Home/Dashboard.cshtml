@{
    ViewData["Title"] = "Dashboard";
}
<h1>@ViewData["Title"]</h1>

<p>Use this page to detail your site's privacy policy.</p>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
    <title>Dashboard</title>

</head>

<body class="text-center flex-fill justify-content-center align-items-center align-content-center align-self-center m-auto" id="page-top">
    <div class="text-center d-flex justify-content-center align-items-center align-content-center align-self-center m-auto justify-content-xxl-center align-items-xxl-center" id="wrapper">
        <div class="d-flex flex-column" id="content-wrapper">
            <div id="content">
                <div class="container-fluid text-center justify-content-center align-items-center align-content-center align-self-center m-auto" style="background: rgb(255,255,255);">
                    <div class="text-center m-auto mb-4">
                        <h1 class="mb-0" style="font-family: Myriad pro;font-size: 60px;">Dashboard</h1>
                    </div>
                    <div class="row justify-content-center align-self-center m-auto">
                        <div class="col-lg-12 col-xl-8">
                            <div class="card shadow mb-4" style="border-radius: 20px;background: linear-gradient(#D6F3FF, #3BC2F8);">
                                <div class="card-header d-flex justify-content-between align-items-center" style="background: rgba(242,32,32,0);border-style: none;">
                                    <h1 style="font-weight: bold;font-family: Myriad pro;">Overall progress</h1>
                                </div>
                                <div class="card-body" style="border-radius: 20px;background: transparent;">
                                    <div class="chart-area" style="color: #2A3282;"><canvas data-bss-chart="{&quot;type&quot;:&quot;line&quot;,&quot;data&quot;:{&quot;labels&quot;:[&quot;Montag&quot;,&quot;Dienstag&quot;,&quot;Mittwoch&quot;,&quot;Donnerstag&quot;,&quot;Freitag&quot;,&quot;Samstag&quot;,&quot;Sonntag&quot;],&quot;datasets&quot;:[{&quot;label&quot;:&quot;Earnings&quot;,&quot;fill&quot;:true,&quot;data&quot;:[&quot;0&quot;,&quot;25&quot;,&quot;130&quot;,&quot;172&quot;,&quot;414&quot;,&quot;515&quot;,&quot;732&quot;,&quot;940&quot;],&quot;backgroundColor&quot;:&quot;rgba(78, 115, 223, 0.05)&quot;,&quot;borderColor&quot;:&quot;#ffffff&quot;,&quot;borderWidth&quot;:&quot;5&quot;}]},&quot;options&quot;:{&quot;maintainAspectRatio&quot;:false,&quot;legend&quot;:{&quot;display&quot;:false,&quot;labels&quot;:{&quot;fontStyle&quot;:&quot;normal&quot;,&quot;fontSize&quot;:54}},&quot;title&quot;:{&quot;fontStyle&quot;:&quot;normal&quot;},&quot;scales&quot;:{&quot;xAxes&quot;:[{&quot;gridLines&quot;:{&quot;color&quot;:&quot;rgb(234, 236, 244)&quot;,&quot;zeroLineColor&quot;:&quot;rgb(234, 236, 244)&quot;,&quot;drawBorder&quot;:false,&quot;drawTicks&quot;:false,&quot;borderDash&quot;:[&quot;2&quot;],&quot;zeroLineBorderDash&quot;:[&quot;2&quot;],&quot;drawOnChartArea&quot;:false},&quot;ticks&quot;:{&quot;fontColor&quot;:&quot;#2A3282&quot;,&quot;padding&quot;:20}}],&quot;yAxes&quot;:[{&quot;gridLines&quot;:{&quot;color&quot;:&quot;rgb(234, 236, 244)&quot;,&quot;zeroLineColor&quot;:&quot;rgb(234, 236, 244)&quot;,&quot;drawBorder&quot;:false,&quot;drawTicks&quot;:false,&quot;borderDash&quot;:[&quot;2&quot;],&quot;zeroLineBorderDash&quot;:[&quot;2&quot;]},&quot;ticks&quot;:{&quot;fontColor&quot;:&quot;#2A3282&quot;,&quot;padding&quot;:20}}]}}}"></canvas></div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col">
                                    <div class="card shadow border-start-info py-2" style="border-radius: 20px;background: transparent;">
                                        <div class="card-body" style="border-radius: 20px;background: linear-gradient(#D6F3FF, #3BC2F8), var(--bs-white);color: #2A3282;margin-bottom: 10px;">
                                            <div class="row align-items-center no-gutters" style="border-radius: 20px;background: transparent;color: #2A3282;">
                                                <div class="col me-2" style="border-radius: 20px;background: transparent;color: #2A3282;">
                                                    <div class="text-uppercase text-info fw-bold text-xs mb-1" style="border-radius: 20px;background: transparent;color: #2A3282;"><span style="border-radius: 20px;background: transparent;color: #2A3282;">Laufen</span></div>
                                                    <div class="row g-0 align-items-center" style="border-radius: 20px;background: transparent;color: #2A3282;">
                                                        <div class="col-auto" style="border-radius: 20px;background: transparent;color: #2A3282;">
                                                            <div class="text-dark fw-bold h5 mb-0 me-3" style="border-radius: 20px;background: transparent;color: #2A3282;"><span style="border-radius: 20px;background: transparent;color: #2A3282;font-family: Myriad pro;">70KM</span></div>
                                                        </div>
                                                        <div class="col" style="border-radius: 20px;background: var(--bs-white);color: #2A3282;">
                                                            <div class="progress border rounded-pill shadow-lg progress-sm" style="border-radius: 20px;background: #2A3282;border-color: var(--bs-red);color: #2A3282;">
                                                                <div class="progress-bar bg-light progress-bar-animated" aria-valuenow="70%" aria-valuemin="0" aria-valuemax="100"><span class="visually-hidden">70%%</span></div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-auto" style="border-radius: 20px;background: transparent;color: #2A3282;"><i class="fas fa-running fa-2x text-gray-300" style="border-radius: 20px;background: transparent;color: #ffffff;"></i></div>
                                            </div>
                                        </div>
                                        <div class="card-body" style="border-radius: 20px;background: linear-gradient(#D6F3FF, #3BC2F8), transparent;color: #2A3282;margin-bottom: 10px;">
                                            <div class="row align-items-center no-gutters" style="border-radius: 20px;background: transparent;color: #2A3282;">
                                                <div class="col me-2" style="border-radius: 20px;background: transparent;color: #2A3282;">
                                                    <div class="text-uppercase text-info fw-bold text-xs mb-1" style="border-radius: 20px;background: transparent;color: #2A3282;"><span style="border-radius: 20px;background: transparent;color: #2A3282;">Schwimmen</span></div>
                                                    <div class="row g-0 align-items-center" style="border-radius: 20px;background: transparent;color: #2A3282;">
                                                        <div class="col-auto" style="border-radius: 20px;background: transparent;color: #2A3282;">
                                                            <div class="text-dark fw-bold h5 mb-0 me-3" style="border-radius: 20px;background: transparent;color: #2A3282;"><span style="border-radius: 20px;background: transparent;color: #2A3282;font-family: Myriad pro;">21KM</span></div>
                                                        </div>
                                                        <div class="col" style="border-radius: 20px;background: var(--bs-white);color: #2A3282;">
                                                            <div class="progress border rounded-pill shadow-lg progress-sm" style="border-radius: 20px;background: #2A3282;border-color: var(--bs-red);color: #2A3282;">
                                                                <div class="progress-bar bg-light progress-bar-animated" aria-valuenow="20%" aria-valuemin="0" aria-valuemax="100"><span class="visually-hidden">20%%</span></div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-auto" style="border-radius: 20px;background: transparent;color: #2A3282;"><i class="fas fa-swimmer fa-2x text-gray-300" style="border-radius: 20px;background: transparent;color: #2A3282;"></i></div>
                                            </div>
                                        </div>
                                        <div class="card-body" style="border-radius: 20px;background: linear-gradient(#D6F3FF, #3BC2F8), transparent;color: #2A3282;margin-bottom: 10px;">
                                            <div class="row align-items-center no-gutters" style="border-radius: 20px;background: transparent;color: #2A3282;">
                                                <div class="col me-2" style="border-radius: 20px;background: transparent;color: #2A3282;">
                                                    <div class="text-uppercase text-info fw-bold text-xs mb-1" style="border-radius: 20px;background: transparent;color: #2A3282;"><span style="border-radius: 20px;background: transparent;color: #2A3282;">Fahrrad</span></div>
                                                    <div class="row g-0 align-items-center" style="border-radius: 20px;background: transparent;color: #2A3282;">
                                                        <div class="col-auto" style="border-radius: 20px;background: transparent;color: #2A3282;">
                                                            <div class="text-dark fw-bold h5 mb-0 me-3" style="border-radius: 20px;background: transparent;color: #2A3282;"><span style="border-radius: 20px;background: transparent;color: #2A3282;font-family: Myriad pro;">123KM</span></div>
                                                        </div>
                                                        <div class="col" style="border-radius: 20px;background: var(--bs-white);color: #2A3282;">
                                                            <div class="progress border rounded-pill shadow-lg progress-sm" style="border-radius: 20px;background: #ffffff;border-color: var(--bs-red);color: #2A3282;">
                                                                <div class="progress-bar bg-warning progress-bar-striped progress-bar-animated" aria-valuenow="70%" aria-valuemin="0" aria-valuemax="100"><span class="visually-hidden">70%%</span></div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-auto" style="border-radius: 20px;background: transparent;color: #2A3282;"><i class="fas fa-biking fa-2x text-gray-300" style="border-radius: 20px;background: transparent;color: #2A3282;"></i></div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="card shadow border-start-primary py-2" style="border-radius: 20px;background: transparent;">
                                <div class="card-body" style="border-radius: 20px;background: linear-gradient(#D6F3FF, #3BC2F8);margin-bottom: 10px;">
                                    <h1 style="font-family: Myriad pro;">Company contribution</h1>
                                    <div style="border-radius: 20px;"><canvas data-bss-chart="{&quot;type&quot;:&quot;bar&quot;,&quot;data&quot;:{&quot;labels&quot;:[&quot;January&quot;,&quot;February&quot;,&quot;March&quot;,&quot;April&quot;,&quot;May&quot;,&quot;June&quot;],&quot;datasets&quot;:[{&quot;label&quot;:&quot;Revenue&quot;,&quot;backgroundColor&quot;:&quot;#2a3282&quot;,&quot;borderColor&quot;:&quot;#9c9c9d&quot;,&quot;borderWidth&quot;:&quot;0&quot;,&quot;data&quot;:[&quot;4500&quot;,&quot;5300&quot;,&quot;6250&quot;,&quot;7800&quot;,&quot;9800&quot;,&quot;15000&quot;]}]},&quot;options&quot;:{&quot;maintainAspectRatio&quot;:true,&quot;legend&quot;:{&quot;display&quot;:false,&quot;labels&quot;:{&quot;bold&quot;:false,&quot;italic&quot;:false,&quot;fontColor&quot;:&quot;#ffffff&quot;,&quot;fontStyle&quot;:&quot;normal&quot;}},&quot;title&quot;:{&quot;fontStyle&quot;:&quot;bold&quot;,&quot;fontColor&quot;:&quot;#ffffff&quot;},&quot;scales&quot;:{&quot;xAxes&quot;:[{&quot;gridLines&quot;:{&quot;drawBorder&quot;:true,&quot;drawTicks&quot;:true,&quot;borderDash&quot;:[&quot;3&quot;],&quot;zeroLineBorderDash&quot;:[&quot;3&quot;],&quot;drawOnChartArea&quot;:false},&quot;ticks&quot;:{&quot;fontColor&quot;:&quot;#2a3282&quot;,&quot;beginAtZero&quot;:true}}],&quot;yAxes&quot;:[{&quot;gridLines&quot;:{&quot;drawBorder&quot;:true,&quot;drawTicks&quot;:true,&quot;borderDash&quot;:[&quot;3&quot;],&quot;zeroLineBorderDash&quot;:[&quot;3&quot;],&quot;drawOnChartArea&quot;:true},&quot;ticks&quot;:{&quot;fontColor&quot;:&quot;#2a3282&quot;,&quot;beginAtZero&quot;:true}}]}}}"></canvas></div>
                                </div>
                                <div class="card-body" style="border-radius: 20px;background: linear-gradient(#D6F3FF, #3BC2F8);margin-bottom: 10px;">
                                    <div class="row align-items-center no-gutters" style="border-radius: 20px;">
                                        <div class="col me-2" style="border-radius: 20px;">
                                            <div class="text-uppercase text-primary fw-bold text-xs mb-1" style="border-radius: 20px;"><span style="border-radius: 20px;font-family: Myriad pro;color: rgb(42,50,130);">Kilometer pro Tag</span></div>
                                            <div class="text-dark fw-bold h5 mb-0" style="border-radius: 20px;"><span style="border-radius: 20px;color: var(--bs-white);">732</span></div>
                                        </div>
                                        <div class="col-auto" style="border-radius: 20px;"><i class="icon ion-ios-speedometer fa-2x text-gray-300" style="border-radius: 20px;"></i></div>
                                    </div>
                                </div>
                                <div class="card-body" style="border-radius: 20px;background: linear-gradient(#D6F3FF, #3BC2F8);">
                                    <div class="row align-items-center no-gutters" style="border-radius: 20px;">
                                        <div class="col me-2" style="border-radius: 20px;">
                                            <div class="text-uppercase text-primary fw-bold text-xs mb-1" style="border-radius: 20px;"><span style="border-radius: 20px;font-family: Myriad pro;color: rgb(42,50,130);">Teilnehmer</span></div>
                                            <div class="text-dark fw-bold h5 mb-0" style="border-radius: 20px;"><span style="border-radius: 20px;border-style: none;color: var(--bs-white);">592</span></div>
                                        </div>
                                        <div class="col-auto" style="border-radius: 20px;"><i class="icon ion-ios-people fa-2x text-gray-300" style="border-radius: 20px;"></i></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div><a class="border rounded d-inline scroll-to-top" href="#page-top"><i class="fas fa-angle-up"></i></a>
    </div>

</body>
